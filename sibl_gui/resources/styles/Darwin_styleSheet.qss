/* sIBL_GUI - Stylesheet - Darwin Os */
/* Application. */
/* QWidget. */
QWidget {
	background-color: rgb(64, 64, 64);
	color: rgb(192, 192, 192);
	selection-background-color: rgb(96, 96, 96);
	selection-color: rgb(224, 224, 224);
}

/* QMainWindow. */
QMainWindow::separator {
	background-color: rgb(32, 32, 32);
	height: 4px;
	width: 4px;
}

QMainWindow::separator:hover {
	background-color: rgb(88, 88, 88);
}

/* Toolbar. */
#toolBar {
	background-color: transparent;
	border: none;
	spacing: 8px;
}

#Application_Logo_label, #Spacer_label, #Library_activeLabel, #Inspect_activeLabel, #Export_activeLabel, #Edit_activeLabel, #Preferences_activeLabel, #Central_Widget_activeLabel, #Custom_Layouts_activeLabel, #Miscellaneous_activeLabel, #Closure_Spacer_label, #qt_toolbar_ext_button {
	background-color: transparent;
}

/* Overall. */
QComboBox, QDateTimeEdit, QDoubleSpinBox, QListView, QLineEdit, QPlainTextEdit, QSpinBox, QTableView, QTreeView, QTextEdit, QTextBrowser {
	background-color: rgb(32, 32, 32);
	border-radius: 0px;
}

/* QCheckBox. */
QCheckBox::indicator {
	background-color: rgb(32, 32, 32);
	color: rgb(88, 88, 88);
	height: 16px;
	width: 16px;
}

/*QCheckBox::indicator:unchecked{
	image: url(images/ComboBox_Cross.png);
}

QCheckBox::indicator:unchecked:hover{
	image: url(images/ComboBox_Cross.png);
}

QCheckBox::indicator:unchecked:pressed{
	image: url(images/ComboBox_Cross.png);
}*/

QCheckBox::indicator:checked{
	image: url(images/ComboBox_Cross.png);
}

QCheckBox::indicator:checked:hover{
	image: url(images/ComboBox_Cross.png);
}

QCheckBox::indicator:checked:pressed{
	image: url(images/ComboBox_Cross.png);
}

QCheckBox::indicator:indeterminate:hover{
	image: url(images/ComboBox_Cross.png);
}

QCheckBox::indicator:indeterminate:pressed{
	image: url(images/ComboBox_Cross.png);
}

/* QComboBox. */
QComboBox {
	background-color: rgb(32, 32, 32);
	border-radius: 0px;
	padding: 4px;
}

QComboBox::drop-down {
	background-color: rgb(88, 88, 88);
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 16px;
}
 
QComboBox::down-arrow {
	image: url(images/Small_Down_Arrow.png);
}

QComboBox::down-arrow:on {
	top: 1px;
	left: 1px;
}

/* QDateTimeEdit, QDoubleSpinBox, QSpinBox. */
QDateTimeEdit, QDoubleSpinBox, QSpinBox {
	padding: 4px;
}

QDateTimeEdit::up-button, QDoubleSpinBox::up-button, QSpinBox::up-button  {
	background-color: rgb(88, 88, 88);
}

QDateTimeEdit::up-arrow, QDoubleSpinBox::up-arrow, QSpinBox::up-arrow  {
	image: url(images/Tiny_Up_Arrow.png);
}

QDateTimeEdit::up-button:hover, QDoubleSpinBox::up-button:hover, QSpinBox::up-button:hover {
	background-color: rgb(96, 96, 96);
}

QDateTimeEdit::up-button:pressed, QDoubleSpinBox::up-button:pressed, QSpinBox::up-button:pressed {
/*	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 rgb(32, 32, 32), stop: 1 rgb(84, 84, 84));*/
	padding: 1px -1px -1px 1px;
}

QDateTimeEdit::down-button, QDoubleSpinBox::down-button, QSpinBox::down-button  {
	background-color: rgb(88, 88, 88);
}

QDateTimeEdit::down-button:hover, QDoubleSpinBox::down-button:hover, QSpinBox::down-button:hover {
	background-color: rgb(96, 96, 96);
}

QDateTimeEdit::down-button:pressed, QDoubleSpinBox::down-button:pressed, QSpinBox::down-button:pressed {
/*	background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 rgb(84, 84, 84), stop: 1 rgb(32, 32, 32));*/
	padding: 1px -1px -1px 1px;
}

QDateTimeEdit::down-arrow, QDoubleSpinBox::down-arrow, QSpinBox::down-arrow  {
	image: url(images/Tiny_Down_Arrow.png);
}

/* QDockWidget. */
QDockWidget {
	titlebar-close-icon: url(images/Close_Cross.png);
	titlebar-normal-icon: url(images/Undock_Window.png);
}

QDockWidget::title {
	background-color: rgb(64, 64, 64);
	padding: 8px;
	text-align: middle;
}

QDockWidget::close-button, QDockWidget::float-button {
	border: 2px solid transparent;
	background-color: rgb(64, 64, 64);
}

QDockWidget::close-button:hover {
	image: url(images/Close_Cross_Hover.png);
}

QDockWidget::float-button:hover {
	image: url(images/Undock_Window_Hover.png);
}

QDockWidget::close-button:pressed, QDockWidget::float-button:pressed {
	padding: 1px -1px -1px 1px;
}

/* QGroupBox. */
QGroupBox {
	border-top: 2px solid rgb(32, 32, 32);
	border-radius: 0px;
	margin-top: 8px;
	padding: 8px 0px 0px 0px;
}

QGroupBox::title {
	padding: 0px 8px;
	subcontrol-origin: margin;
	subcontrol-position: top center;
}

/* QHeaderView. */
QHeaderView::section {
	background-color: rgb(64, 64, 64);
	border-bottom: 2px solid rgb(32, 32, 32);
	border-radius: 0px;
	border-top: 2px solid rgb(32, 32, 32);
	padding: 4px;
}

QHeaderView::section:horizontal {
	border-left: 1px solid rgb(32, 32, 32);
	border-right: 1px solid rgb(32, 32, 32);
}

QHeaderView::section:vertical {
	border-top: 1px solid rgb(32, 32, 32);
	border-bottom: 1px solid rgb(32, 32, 32);
}
 
QHeaderView::section:checked {
	background-color: rgb(96, 96, 96);
}

QHeaderView::down-arrow {
	image: url(images/Small_Down_Arrow.png);
}

QHeaderView::up-arrow {
	image: url(images/Small_Up_Arrow.png);
}

/* QLineEdit. */
QLineEdit {
	padding: 4px;
}

/* QListView. */
QListView::item {
	background-color: rgb(48, 48, 48);
	border: 2px solid rgb(32, 32, 32);
	padding: 4px;
}

QListView::item:selected:!active {
	background-color: rgb(96, 96, 96);
}

QListView::item:selected:active {
	background-color: rgb(96, 96, 96);
}

QListView::item:hover {
	background-color: rgb(64, 64, 64);
}

/* QPushButton, QToolButton. */
QPushButton, QToolButton {
	background-color: rgb(88, 88, 88);
	border-radius: 0px;
	padding: 8px;
}

QPushButton::hover, QToolButton::hover {
	background-color: rgb(96, 96, 96);
}

QPushButton::pressed, QToolButton::pressed {
	background-color: rgb(32, 32, 32);
}

/* QSizeGrip. */
QSizeGrip {
	height: 16px;
	background-color: transparent;
	background-image: url(images/Resize_Grip.png);
	width: 16px;
}

/* QScrollBar. */
QScrollBar:horizontal {
	background-color: rgb(32, 32, 32);
	border: 2px solid rgb(32, 32, 32);
	height: 12px;
}

QScrollBar:vertical {
	background-color: rgb(32, 32, 32);
	border: 2px solid rgb(32, 32, 32);
	width: 12px;
}

QScrollBar:horizontal:hover, QScrollBar:horizontal:pressed, QScrollBar:vertical:hover, QScrollBar:vertical:pressed {
	background-color: rgb(48, 48, 48);
	border: 2px solid rgb(48, 48, 48);
}

QScrollBar::handle:horizontal {
	background-color: rgb(88, 88, 88);
	min-width: 24px;
}

QScrollBar::handle:horizontal:hover {
	background-color: rgb(96, 96, 96);
}

QScrollBar::handle:horizontal:pressed {
	background-color: rgb(32, 32, 32);
}

QScrollBar::handle:vertical {
	background-color: rgb(88, 88, 88);
	min-height: 24px;
}

QScrollBar::handle:vertical:hover {
	background-color: rgb(96, 96, 96);
}

QScrollBar::handle:vertical:pressed {
	background-color: rgb(32, 32, 32);
}

QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal, QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
	background: none;
	height: 0px;
	width: 0px;
}

QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal, QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	background: none;
	height: 0px;
	width: 0px;
}

/* QSlider. */
QSlider::groove:horizontal {
	background-color: rgb(32, 32, 32);
	border: 2px solid rgb(32, 32, 32);
	height: 8px;
	margin: 0px;
}

QSlider::groove:hover {
	background-color: rgb(48, 48, 48);
	border: 2px solid rgb(48, 48, 48);
}

QSlider::handle {
	background-color: rgb(88, 88, 88);
	border: 0px;
	width: 24px;
}

QSlider::handle:hover {
	background-color: rgb(96, 96, 96);
}

QSlider::handle:pressed {
	background-color: rgb(32, 32, 32);
}

/* QSplitter. */
QSplitter::handle {
	background-color: rgb(32, 32, 32);
}

QSplitter::handle:horizontal {
	width: 4px;
}

QSplitter::handle:vertical {
	height: 4px;
}

QSplitter::handle:hover {
	background-color: rgb(88, 88, 88);
}

QSplitter::handle:pressed {
	background-color: rgb(32, 32, 32);
}

/* QStatusBar. */
QStatusBar {
	background-color: rgb(64, 64, 64);
	border-top: 4px solid rgb(32, 32, 32);
}

QStatusBar::item {
	border: 0px transparent;
}

/* QTableView. */
QTableView {
	gridline-color: rgb(32, 32, 32);
}

/* QTabBar. */
QTabBar::tab {
	background-color: rgb(64, 64, 64);
	margin-right: 4px;
	padding: 4px;
}

QTabBar::tab:hover {
	background-color: rgb(96, 96, 96);
}

QTabBar::tab:selected {
	background-color: rgb(88, 88, 88);
}

QTabBar::tab:last:selected {
	margin-right: 4px;
}
 
QTabBar::close-button {
	image: url(images/Close_Cross.png);
}

QTabBar::close-button:hover {
	image: url(images/Close_Cross_Hover.png);
}

QTabBar::close-button:pressed {
	padding: 1px -1px -1px 1px;
}

/* QTabWidget. */
QTabWidget::pane {

}

/* QToolBox. */
QToolBox::tab {
	background-color: rgb(88, 88, 88);
}

QToolBox::tab:selected {
	background-color: rgb(96, 96, 96);
	font: inherit;
}

QToolBox::tab:hover {
	background-color: rgb(96, 96, 96);
}

QToolBox::tab:pressed {
	background-color: rgb(32, 32, 32);
}

/* QToolTip. */
QToolTip {
	background-color: rgb(64, 64, 64);
	border: 0px transparent;
	color: rgb(192, 192, 192);
	opacity: 200;
	padding: 8px;
}

/* QTreeView. */
QTreeView::branch:has-siblings:!adjoins-item {
	border-image: url(images/TreeView_VLine.png) 0;
}

QTreeView::branch:has-siblings:adjoins-item {
	border-image: url(images/TreeView_Branch_More.png) 0;
}

QTreeView::branch:!has-children:!has-siblings:adjoins-item {
	border-image: url(images/TreeView_Branch_End.png) 0;
}

QTreeView::branch:has-children:!has-siblings:closed, QTreeView::branch:closed:has-children:has-siblings {
	border-image: none;
	image: url(images/TreeView_Branch_Closed.png);
}

QTreeView::branch:open:has-children:!has-siblings,QTreeView::branch:open:has-children:has-siblings  {
	border-image: none;
	image: url(images/TreeView_Branch_Open.png);
}

QTreeView::branch:hover, QTreeView::item:hover {
	background-color: rgb(64, 64, 64);
}

/* Search_QlineEdit. */
#Search_Field_activeLabel, #Clear_Field_button {
	background-color: transparent;
}

/* Components. */
/* Factory Component "Script Editor". */
#Script_Editor_tabWidget {
	border: 0px;
}

#Lines_Numbers_textEdit {
	background-color: rgb(88, 88, 88);
}

#Lines_Columns_label {
	margin: 4px 8px 4px 8px;
}

#Search_Results_treeView::branch:has-siblings:!adjoins-item {
	border-image: none;
}

#Search_Results_treeView::branch:has-siblings:adjoins-item {
	border-image: none;
}

#Search_Results_treeView::branch:!has-children:!has-siblings:adjoins-item {
	border-image: none;
}

/* Factory Component "Projects Explorer". */
#Projects_Explorer_treeView::branch:has-siblings:!adjoins-item {
	border-image: none;
}

#Projects_Explorer_treeView::branch:has-siblings:adjoins-item {
	border-image: none;
}

#Projects_Explorer_treeView::branch:!has-children:!has-siblings:adjoins-item {
	border-image: none;
}

/* Core Component "Inspector". */
#Inspector_Overall_frame {
	background-color: rgb(32, 32, 32);
}

#Image_frame {
	padding: 8px;
}

/* Addon Component "Search Database". */
#Tags_Cloud_listWidget::item {
	border: 0px;
	padding: 2px;
}
